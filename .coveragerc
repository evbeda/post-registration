[run]
branch = True
plugins =
    django_coverage_plugin
source =
  documentsManager
  post_registration
  templates
omit =
    env/*
    */test*
    manage.py
    post_registration/wsgi.py
    kazienenv/*
    */tests/*,
    */migrations/*,
    */middleware.py,
    */urls.py,
    */settings/*,
    */wsgi.py,
    manage.py,
    fabfile.py,
    */app_settings.py,
[report]
show_missing =  True
exclude_lines =
    pragma: no cover
    def setUp
    def test_
    return raw_input
    if __name__ == '__main__':

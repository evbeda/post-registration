[run]
source =
  documentsManager
  post_registration
  templates
omit =
    env/*
    */test*
    manage.py
    post_registration/wsgi.py
    kazienenv/*
    */tests/*,
    */migrations/*,
    */urls.py,
    */settings/*,
    */wsgi.py,
    manage.py,
    fabfile.py

[report]
exclude_lines =
    pragma: no cover
    def setUp
    def test_
    return raw_input
    if __name__ == '__main__':
